            android:label="@string/activity_detalhes_participante" />
        <activity
            android:name=".CadastroLivro"
            android:label="@string/activity_cadastrar_livro" />
        <activity
            android:name=".VisualizarLivros"
            android:label="@string/activity_visualizar_livros" />
        <activity
            android:name=".CadastroReserva"
            android:label="@string/activity_cadastrar_reserva" />
